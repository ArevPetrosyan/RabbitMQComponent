{
  "AppSettings": {
    "FeedconstructRMQ": {
      "QueueName": "",
      "Exchange": "",
      "Hostname": "",
      "Port": 5672,
      "VHost": "",
      "Username": "",
      "Password": ""
    },

    "FilteringCount": 2,
    "ApiKey": "",
    "RestURL": "",
    "SqlConnectionString": "Server=constring",
    "RedisPort": 6379,
    "RedisHost": "",
    "RedisPassword": "",
    "ApplicationName": "",
    "TranslationBaseURL": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ColoredConsole",
      "Serilog.Sinks.RollingFileAlternative",
      "Serilog.Sinks.Udp",
      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Udp",
        "Args": {
          "remoteAddress": "127.0.0.1",
          "remotePort": 7071
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "c:/Logs/BetcoClient/{Date}-{Level}.log",
          "bufferSize": 5120,
          "retainedFileDurationLimit": "2.00:00:00",
          "supportAsync": true
        }
      },
      {
        "Name": "ColoredConsole",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://ELK_ADDRESS/",
          "indexFormat": "feedco-{0:yyyy.MM}",
          "connectionGlobalHeaders": "Authorization=Basic ELK_PASS",
          "autoRegisterTemplate": true
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "BetcoClient"
    }
  }
}
